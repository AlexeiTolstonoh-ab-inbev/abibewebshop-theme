{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": {
    "props": {
      "blockClass": "footer-container"
    },
    "children": [
      "flex-layout.row#footer-1-desktop"
    ]
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "vtex.menu@2.x:menu#footer-politics",
      "flex-layout.col#info",
      "flex-layout.col#payment-methods"
    ],
    "props": {
      "blockClass": "footer-menu-row",
      "paddingTop": 6,
      "paddingBottom": 6
    }
  },

  "flex-layout.col#info": {
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "top"
    },
    "children": ["rich-text#information","social-networks"]
  },
  "rich-text#information": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "Follow us:",
      "blockClass": "footerSocialHeader",
      "font": "t-heading-2"
    }
  },
  "social-networks": {
    "props": {
      "socialNetworks": [
        {
          "name": "Facebook",
          "url": "https://www.facebook.com/jupiler"
        },
        {
          "name": "Instagram",
          "url": "https://contactus.ab-inbev.com/fr"
        }
      ]
    }
  },
  "flex-layout.col#payment-methods": {
    "props": {
      "horizontalAlign": "right",
      "verticalAlign": "bottom"
    },
    "children":["accepted-payment-methods"]
  },
  "accepted-payment-methods": {
    "props": {
      "paymentMethods": ["MasterCard", "Visa", "Diners Club"]
    }
  },



  "footer-layout.mobile": {
    "children": [
      "flex-layout.row#1-footer-mobile",
      "flex-layout.row#2-footer-mobile",
      "flex-layout.row#3-footer-mobile"
    ]
  },
  "flex-layout.row#3-footer-mobile": {
    "children": ["flex-layout.col#footer-2-mobile"],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4
    }
  },
  "flex-layout.col#footer-2-mobile": {
    "children": [
      "rich-text#copyrightmobile", "rich-text#responseDrinkmobile"
    ],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4,
      "rowGap": 4
    }
  },  "rich-text#copyrightmobile": {
  "props": {
    "textAlignment": "CENTRE",
    "textPosition": "CENTRE",
    "text": "Â© 2021 AB InBev UK Limited T/A Budweiser Brewing Group UK&I.ALL RIGHTS RESERVED.\nPLEASE DO NOT SHARE OR FORWARD THIS WEBSITE OR ITS CONTENTS TO ANYONE UNDER 18",
    "font": "heading",
    "blockClass": "copyrightFooter"
  }
},
  "rich-text#responseDrinkmobile": {
    "props": {
      "textAlignment": "CENTRE",
      "textPosition": "CENTRE",
      "text": "[www.drinkaware.co.uk](http://www.drinkaware.co.uk)\nPLEASE DRINK RESPONSIBLY",
      "blockClass": "copyrightResponse",
      "font": "heading"
    }
  },
  "flex-layout.row#2-footer-mobile": {
    "children": ["flex-layout.col#footer-1-mobile"],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4
    }
  },
  "flex-layout.col#footer-1-mobile": {
    "children": [
      "accepted-payment-methods",
      "social-networks"
    ],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4,
      "rowGap": 4
    }
  },
  "flex-layout.row#1-footer-mobile": {
    "props": {
      "paddingTop": 4,
      "paddingBottom": 4
    },
    "children": ["vtex.menu@2.x:menu#footer-mobile"]
  }
}
