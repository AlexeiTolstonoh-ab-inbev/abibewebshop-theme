{
  "flex-layout.row#features-wrapper": {
  "props": {
    "blockClass": "features-wrapper",
    "horizontalAlign": "center",
    "preventVerticalStretch": true,
    "preserveLayoutOnMobile": true
  },
    "children": ["flex-layout.col#col1", "flex-layout.col#col2", "flex-layout.col#col3", "flex-layout.col#col4"]
  },

  "flex-layout.col#col1": {
    "props": {
      "blockClass": "features-column",
      "horizontalAlign": "center"
    },
    "children": ["image#collage-image1","rich-text#features-text1"]
  },
  "flex-layout.col#col2": {
