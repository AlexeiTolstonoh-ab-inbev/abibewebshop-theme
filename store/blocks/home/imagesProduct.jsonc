"flex-layout.row#puerbm-container":{
"props":{
"blockClass": "puerbm-container",
"horizontalAlign":"center",
"fullWidth": true
},
"children":["flex-layout.col#puerbm-wrapper"]
},
"flex-layout.col#puerbm-wrapper":{
  "children":["rich-text#toptext","flex-layout.row#bottomimgrow","flex-layout.row#bottomlinkrow"]
},


  "rich-text#toptext":{
    "props":{
    "blockClass": "puerbm-title",
         "textAlignment": "CENTER",
          "textPosition": "CENTER",
          "text": "Our brands"
          }
          },




"flex-layout.row#bottomimgrow":{
  "props":{
  "fullWidth": true,
  "horizontalAlign":"center",
  "blockClass": "bottomimgcontainer",
  "preserveLayoutOnMobile": true
  },
  "children":["slider-layout#puerbmSlider"]
  },

  "slider-layout#puerbmSlider": {
    "props": {
      "itemsPerPage": {
        "desktop": 8,
        "tablet": 4,
        "phone": 2
      },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "blockClass": "carousel"
    },
    "children": ["flex-layout.col#img1","flex-layout.col#img2","flex-layout.col#img3",
                               "flex-layout.col#img4", "flex-layout.col#img5","flex-layout.col#img6", "flex-layout.col#img7","flex-layout.col#img8"]

  },

"flex-layout.row#bottomlinkrow":{
  "props":{
  "fullWidth": true,
  "blockClass": "bottomlinkcontainer",
  "horizontalAlign":"center"
  },
  "children":["rich-text#bottomlink"]
},

